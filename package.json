{
	"name": "light-whale",
	"version": "0.2.5",
	"scripts": {
		"openapi-gen": "openapi-generator-cli generate",
		"postinstall": "npm run openapi-gen",
		"dev": "vite dev",
		"build": "vite build && rimraf -g \"./build/**/*.map\"",
		"postbuild": "cd build && npm init -y && npm pkg set name=light-whale type=module license=MIT scripts.start=\"node index.js\"",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@openapitools/openapi-generator-cli": "^2.5.2",
		"@poppanator/sveltekit-svg": "^3.0.0",
		"@sveltejs/adapter-node": "^1.2.3",
		"@sveltejs/kit": "^1.15.1",
		"@tailwindcss/typography": "^0.5.9",
		"@types/highlightjs": "^9.12.2",
		"@types/marked": "^4.0.8",
		"@types/path-browserify": "^1.0.0",
		"@types/uuid": "^9.0.1",
		"@types/validator": "^13.7.14",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"ansi-to-html": "^0.7.2",
		"autoprefixer": "^10.4.14",
		"daisyui": "^2.51.5",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"heroicons": "^2.0.17",
		"highlight.js": "^11.7.0",
		"linguist-js": "^2.5.4",
		"marked": "^4.3.0",
		"path-browserify": "^1.0.1",
		"postcss": "^8.4.21",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"rimraf": "^4.4.1",
		"sass": "^1.60.0",
		"simple-git": "^3.17.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tailwindcss": "^3.3.1",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"uuid": "^9.0.0",
		"validator": "^13.9.0",
		"vite": "^4.2.1",
		"yaml": "^2.2.1"
	},
	"type": "module"
}
